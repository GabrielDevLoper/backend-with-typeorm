    "uuidv4": "^6.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.5",
    "@types/mocha": "^8.0.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.24",
    "babel-plugin-module-resolver": "^4.0.0",
    "jest": "^26.1.0",
    "ts-jest": "^26.1.3",
    "ts-node": "3.3.0",
    "ts-node-dev": "^1.0.0-pre.52",
